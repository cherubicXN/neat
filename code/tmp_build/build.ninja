ninja_required_version = 1.3
cxx = c++
nvcc = /usr/local/cuda/bin/nvcc

cflags = -DTORCH_EXTENSION_NAME=_hash_encoder -DTORCH_API_INCLUDE_EXTENSION_H -DPYBIND11_COMPILER_TYPE=\"_gcc\" -DPYBIND11_STDLIB=\"_libstdcpp\" -DPYBIND11_BUILD_ABI=\"_cxxabi1011\" -isystem /home/xn/anaconda3/envs/volsdf/lib/python3.9/site-packages/torch/include -isystem /home/xn/anaconda3/envs/volsdf/lib/python3.9/site-packages/torch/include/torch/csrc/api/include -isystem /home/xn/anaconda3/envs/volsdf/lib/python3.9/site-packages/torch/include/TH -isystem /home/xn/anaconda3/envs/volsdf/lib/python3.9/site-packages/torch/include/THC -isystem /usr/local/cuda/include -isystem /home/xn/anaconda3/envs/volsdf/include/python3.9 -D_GLIBCXX_USE_CXX11_ABI=0 -fPIC -std=c++14 -O3 -std=c++14
post_cflags = 
cuda_cflags = -DTORCH_EXTENSION_NAME=_hash_encoder -DTORCH_API_INCLUDE_EXTENSION_H -DPYBIND11_COMPILER_TYPE=\"_gcc\" -DPYBIND11_STDLIB=\"_libstdcpp\" -DPYBIND11_BUILD_ABI=\"_cxxabi1011\" -isystem /home/xn/anaconda3/envs/volsdf/lib/python3.9/site-packages/torch/include -isystem /home/xn/anaconda3/envs/volsdf/lib/python3.9/site-packages/torch/include/torch/csrc/api/include -isystem /home/xn/anaconda3/envs/volsdf/lib/python3.9/site-packages/torch/include/TH -isystem /home/xn/anaconda3/envs/volsdf/lib/python3.9/site-packages/torch/include/THC -isystem /usr/local/cuda/include -isystem /home/xn/anaconda3/envs/volsdf/include/python3.9 -D_GLIBCXX_USE_CXX11_ABI=0 -D__CUDA_NO_HALF_OPERATORS__ -D__CUDA_NO_HALF_CONVERSIONS__ -D__CUDA_NO_BFLOAT16_CONVERSIONS__ -D__CUDA_NO_HALF2_OPERATORS__ --expt-relaxed-constexpr -gencode=arch=compute_86,code=compute_86 -gencode=arch=compute_86,code=sm_86 --compiler-options '-fPIC' -O3 -std=c++14 -allow-unsupported-compiler -U__CUDA_NO_HALF_OPERATORS__ -U__CUDA_NO_HALF_CONVERSIONS__ -U__CUDA_NO_HALF2_OPERATORS__
cuda_post_cflags = 
ldflags = -shared -L/home/xn/anaconda3/envs/volsdf/lib/python3.9/site-packages/torch/lib -lc10 -lc10_cuda -ltorch_cpu -ltorch_cuda_cu -ltorch_cuda_cpp -ltorch -ltorch_python -L/usr/local/cuda/lib64 -lcudart

rule compile
  command = $cxx -MMD -MF $out.d $cflags -c $in -o $out $post_cflags
  depfile = $out.d
  deps = gcc

rule cuda_compile
  depfile = $out.d
  deps = gcc
  command = $nvcc  $cuda_cflags -c $in -o $out $cuda_post_cflags

rule link
  command = $cxx $in $ldflags -o $out

build hashencoder.cuda.o: cuda_compile /home/xn/repo/hawp2vision/volsdf/code/hashencoder/src/hashencoder.cu
build bindings.o: compile /home/xn/repo/hawp2vision/volsdf/code/hashencoder/src/bindings.cpp

build _hash_encoder.so: link hashencoder.cuda.o bindings.o

default _hash_encoder.so

